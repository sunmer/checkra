{
  "name": "checkra",
  "version": "1.0.7",
  "description": "Checkra - Collaboratively improve any website with AI",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunmer/checkra.git"
  },
  "type": "module",
  "main": "./dist/checkra.umd.cjs",
  "module": "./dist/checkra.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/checkra.js",
      "require": "./dist/checkra.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite dev demo",
    "build:lib": "vite build --mode production",
    "build:demo": "npm run build:lib && vite build --mode preprod && cp dist/checkra.umd.cjs demo-dist/ && cp demo/privacy.html demo-dist/",
    "preview:demo": "vite preview --mode preprod --open",
    "preprod": "npm run build:lib && npm run build:demo && npm run preview:demo",
    "publish-checkra": "npm run build:lib && npm version patch && npm publish",
    "build": "npm run build:lib",
    "lint": "eslint .",
    "test": "jest"
  },
  "keywords": [
    "feedback",
    "ai",
    "website improvement",
    "collaboration",
    "frontend",
    "developer-tools"
  ],
  "author": "Sunmer <contact@checkra.io>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.19.86",
    "eslint": "^8.38.0",
    "typescript": "^5.0.0",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^2.3.0",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/postcss": "^4.0.14",
    "@tailwindcss/typography": "^0.5.16"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "marked": "^15.0.8",
    "preline": "^3.0.1"
  }
}
